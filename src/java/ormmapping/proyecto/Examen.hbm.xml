<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="proyecto.Examen" table="examen" schema="proyecto" lazy="false" mutable="false">
		<id name="idusu" column="idusu" type="integer" unsaved-value="0">
			<generator class="native">
				<param name="schema">proyecto</param>
			</generator>
		</id>
		<property name="nombre" column="nombre" type="string" length="40" not-null="false" lazy="false"/>
		<property name="apellido1" column="apellido1" type="string" length="40" not-null="false" lazy="false"/>
		<property name="apellido2" column="apellido2" type="string" length="40" not-null="false" lazy="false"/>
		<property name="nota" column="nota" type="short" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
