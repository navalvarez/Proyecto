CREATE TABLE proyecto2019.banco (idbanco serial NOT NULL, enunciado varchar(300) NOT NULL, imagen varchar(60), tema int2 NOT NULL, subtema int2, dificultad char(1) NOT NULL, activo bool NOT NULL, iddicta int4, idtipo int4, dictaidusu int4, CONSTRAINT banco_pkey PRIMARY KEY (idbanco));
COMMENT ON TABLE proyecto2019.banco IS 'Banco de preguntas o enunciados para las evaluaciones';
COMMENT ON COLUMN proyecto2019.banco.idbanco IS 'Identificador de cada pregunta del banco de preguntas';
COMMENT ON COLUMN proyecto2019.banco.enunciado IS 'Texto del pregunta o enunciado';
COMMENT ON COLUMN proyecto2019.banco.imagen IS 'ruta y nombre de la imagen que acompaña al enunciado';
COMMENT ON COLUMN proyecto2019.banco.tema IS 'Tema de la materia al que corresponde el enunciado';
COMMENT ON COLUMN proyecto2019.banco.subtema IS 'Subtítulo del tema';
COMMENT ON COLUMN proyecto2019.banco.dificultad IS 'Grado de dificultad de la pregunta: 1 a 5';
COMMENT ON COLUMN proyecto2019.banco.activo IS 'True, vigente. False, dado de baja';
COMMENT ON COLUMN proyecto2019.banco.iddicta IS 'Referencia a la tabla Dicta';
COMMENT ON COLUMN proyecto2019.banco.idtipo IS 'Referencia a la tabla Tipo';
COMMENT ON COLUMN proyecto2019.banco.dictaidusu IS 'Referencia a la tabla dicta';
CREATE TABLE proyecto2019.carreras (idcarr serial NOT NULL, nombre varchar(40) NOT NULL, direccion varchar(60), telefono varchar(15), activo bool NOT NULL, CONSTRAINT carreras_pkey PRIMARY KEY (idcarr));
COMMENT ON TABLE proyecto2019.carreras IS 'Contiene las carreras registradas en el sistema';
COMMENT ON COLUMN proyecto2019.carreras.idcarr IS 'Identificador de la carrera';
COMMENT ON COLUMN proyecto2019.carreras.nombre IS 'Nombre de la carrera';
COMMENT ON COLUMN proyecto2019.carreras.direccion IS 'Dirección de la carrera';
COMMENT ON COLUMN proyecto2019.carreras.telefono IS 'Teléfono principal de la carrera';
COMMENT ON COLUMN proyecto2019.carreras.activo IS 'True, vigente. False, dado de baja';
CREATE TABLE proyecto2019.datos (idusu int4 NOT NULL, login varchar(15) NOT NULL, password varchar(100) NOT NULL, activo bool NOT NULL, CONSTRAINT datos_pkey PRIMARY KEY (idusu, login));
COMMENT ON TABLE proyecto2019.datos IS 'Login y contraseña de cada usuario registrado';
COMMENT ON COLUMN proyecto2019.datos.idusu IS 'Referencia a la tabla Usuarios';
COMMENT ON COLUMN proyecto2019.datos.login IS 'Nombre de acceso al sistema del usuario';
COMMENT ON COLUMN proyecto2019.datos.password IS 'Clave encriptada del usuario';
COMMENT ON COLUMN proyecto2019.datos.activo IS 'True, vigente. False, dado de baja';
CREATE TABLE proyecto2019.dicta (iddicta serial NOT NULL, idusu int4 NOT NULL, idcarr int4, sigla char(6), gestion char(4), CONSTRAINT dicta_pkey PRIMARY KEY (iddicta, idusu));
COMMENT ON TABLE proyecto2019.dicta IS 'Asignación de materias a docentes';
COMMENT ON COLUMN proyecto2019.dicta.iddicta IS 'Identificador de la asignación de materias';
COMMENT ON COLUMN proyecto2019.dicta.idusu IS 'Referencia a la tabla Usuarios';
COMMENT ON COLUMN proyecto2019.dicta.idcarr IS 'Referencia a la tabla Materias';
COMMENT ON COLUMN proyecto2019.dicta.sigla IS 'Referencia a la tabla Materias';
COMMENT ON COLUMN proyecto2019.dicta.gestion IS 'Año que se dicta la materia';
CREATE TABLE proyecto2019.docentes (idusu int4 NOT NULL, auxiliar bool NOT NULL, CONSTRAINT docentes_pkey PRIMARY KEY (idusu));
COMMENT ON TABLE proyecto2019.docentes IS 'Tabla de los docentes';
COMMENT ON COLUMN proyecto2019.docentes.idusu IS 'Referencia a la tabla Usuarios';
COMMENT ON COLUMN proyecto2019.docentes.auxiliar IS 'true, auxiliar de cátedra; false, docente';
CREATE TABLE proyecto2019.evaluaciones (idprog int4 NOT NULL, idexamen int4 NOT NULL, nota int2, CONSTRAINT evaluaciones_pkey PRIMARY KEY (idprog, idexamen));
COMMENT ON TABLE proyecto2019.evaluaciones IS 'Contiene los examenes asignados a cada uniersitario';
COMMENT ON COLUMN proyecto2019.evaluaciones.idprog IS 'Referencia a la tabla Programacion';
COMMENT ON COLUMN proyecto2019.evaluaciones.idexamen IS 'Referencia a la tabla Examenes';
COMMENT ON COLUMN proyecto2019.evaluaciones.nota IS 'Nota del examen';
CREATE TABLE proyecto2019.examenes (idexamen serial NOT NULL, nombre varchar(15) NOT NULL, fecha date, hora_ini time(6), hora_fin time(6), penalizacion int2 NOT NULL, activo bool NOT NULL, iddicta int4, dictaidusu int4, CONSTRAINT examenes_pkey PRIMARY KEY (idexamen));
COMMENT ON TABLE proyecto2019.examenes IS 'Evaluaciones preparadas por un docente';
COMMENT ON COLUMN proyecto2019.examenes.idexamen IS 'Identificador del examen';
COMMENT ON COLUMN proyecto2019.examenes.nombre IS 'Nombre del examen';
COMMENT ON COLUMN proyecto2019.examenes.fecha IS 'Fecha del examen';
COMMENT ON COLUMN proyecto2019.examenes.hora_ini IS 'Hora de inicio del examen';
COMMENT ON COLUMN proyecto2019.examenes.hora_fin IS 'Hora de finalización del examen';
COMMENT ON COLUMN proyecto2019.examenes.penalizacion IS 'porcentaje de penalización a las respuestas incorrectas';
COMMENT ON COLUMN proyecto2019.examenes.activo IS 'True, vigente. False, dado de baja';
COMMENT ON COLUMN proyecto2019.examenes.iddicta IS 'Referencia a la tabla dicta';
CREATE TABLE proyecto2019.materias (idcarr int4 NOT NULL, sigla char(6) NOT NULL, nombre varchar(40) NOT NULL, cargahoraria int2 NOT NULL, "plan" varchar(4) NOT NULL, periodo char(1) NOT NULL, paralelo char(1) NOT NULL, activo bool NOT NULL, CONSTRAINT materias_pkey PRIMARY KEY (idcarr, sigla));
COMMENT ON TABLE proyecto2019.materias IS 'Contiene las materias registradas en el sistema';
