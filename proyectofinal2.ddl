COMMENT ON COLUMN proyecto2019.mepro.idmenu IS 'Identificador del menú';
COMMENT ON COLUMN proyecto2019.mepro.idpro IS 'Referencia a la tabla Procesos';
CREATE TABLE proyecto2019.opciones (idopcion serial NOT NULL, idbanco int4, opcion varchar(100) NOT NULL, correcta bool NOT NULL, CONSTRAINT opciones_pkey PRIMARY KEY (idopcion));
COMMENT ON TABLE proyecto2019.opciones IS 'Contiene las diferentes opciones de respuesta para cada pregunta del banco de preguntas';
COMMENT ON COLUMN proyecto2019.opciones.idopcion IS 'Identifcador de las opciones';
