COMMENT ON COLUMN proyecto2019.materias.cargahoraria IS 'Carga horaria (hrs/semana)';
COMMENT ON COLUMN proyecto2019.materias."plan" IS 'Año del plan de estudios en vigencia';
COMMENT ON COLUMN proyecto2019.materias.periodo IS '1, Primer semestre; 2, Segundo semestre; A, Anual; N, Nivelación; V, Verano';
COMMENT ON COLUMN proyecto2019.materias.paralelo IS 'Paralelo de la materia';
COMMENT ON COLUMN proyecto2019.materias.activo IS 'True, vigente. False, dado de baja';
CREATE TABLE proyecto2019.menus (idmenu serial NOT NULL, nombre varchar(40) NOT NULL, activo bool NOT NULL, CONSTRAINT menus_pkey PRIMARY KEY (idmenu));
